{
  "extends": ["//"],
  "pipeline": {
    "build": {
      "outputs": ["./dist/**"],
      "cache": true
    },
    "dev": {
      "cache": true,
      "inputs": ["src/**", "./**"]
    },
    "setup": {
      "cache": true
    },
    "clean-up": {
      "cache": false
    },
    "test": {
      "cache": true,
      "inputs": ["tests/**", ".env"],
      "dependsOn": ["setup"]
    },
    "lint": {},
    "format": {},
    "check": {}
  }
}
